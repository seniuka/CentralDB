  <DataSources>
    <DataSource Name="CentralDB">
      <DataSourceReference>/SQLInventoryReports/SQLInventoryReports</DataSourceReference>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>fdd3daa4-8b8c-461c-bbe7-f06b67dcd68a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="NoDBBackups">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Environment">
            <Value>=Parameters!Environment.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT z.InstanceName, z.DBName,  z.DBSizeInMB, z.RecoveryModel, v.environment, z.LastBackupDate
FROM (select y.* from    (SELECT InstanceName, MAX(DateAdded) AS Rundate
                  FROM      DB.DatabaseInfo
                  GROUP BY InstanceName) AS x INNER JOIN
                  [CentralDB].DB.DatabaseInfo AS y ON x.Rundate = y.DateAdded AND x.InstanceName = y.InstanceName)z
inner join(
select Distinct ServerName, InstanceName, Environment, Description, BusinessOwner from  [CentralDB].[Svr].[ServerList]) v 
ON Z.InstanceName = V.InstanceName AND DATEDIFF(DAY, REPLACE(z.LastBackupDate, 'System.Object[]', '1900-01-01 00:00:00.000'), GETDATE()) &gt; 14 AND z.DBName &lt;&gt; 'TempDB' and Z.DBName Not like '%snapshot%'
Where v.Environment  In (@Environment)
Order By Z.InstanceName</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBName">
          <DataField>DBName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBSizeInMB">
          <DataField>DBSizeInMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="RecoveryModel">
          <DataField>RecoveryModel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="environment">
          <DataField>environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastBackupDate">
          <DataField>LastBackupDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Environ">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <CommandText>SELECT Distinct  [Environment]
  FROM [CentralDB].[Svr].[ServerList]
  Group By Environment
  Order By Environment</CommandText>
      </Query>
      <Fields>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
