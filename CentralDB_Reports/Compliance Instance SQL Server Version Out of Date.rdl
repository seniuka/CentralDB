  <DataSets>
    <DataSet Name="SQLUpdates">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Environment">
            <Value>=Parameters!Environment.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select z.InstanceName, z.SQLVersion, z.SQLPatchLevel + ' (' + SQLVersionNo + ')' SQLPatchLevel, z.SQLEdition, v.Environment from (
select  y.* from(
select InstanceName, Max(DateAdded) as Rundate 
from [CentralDB].[Inst].[InstanceInfo]
Group BY InstanceName) x
Join [CentralDB].[Inst].[InstanceInfo] y ON x.Rundate = y.DateAdded and X.InstanceName = y.InstanceName) z
inner join(
select Distinct ServerName, InstanceName, Environment, Description, BusinessOwner from  [CentralDB].[Svr].[ServerList]) v 
ON Z.InstanceName = V.InstanceName and z.IsSPUpToDate='False'
Where V.Environment IN (@Environment)
Order by z.InstanceName</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLVersion">
          <DataField>SQLVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLPatchLevel">
          <DataField>SQLPatchLevel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SQLEdition">
          <DataField>SQLEdition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ASUpdates">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Environment">
            <Value>=Parameters!Environment.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select z.InstanceName, z.ASVersion, z.ASPatchLevel, z.ASEdition,  v.Environment from (
select  y.* from(
select InstanceName, Max(DateAdded) as Rundate 
from [CentralDB].[AS].[SSASInfo]
Group BY InstanceName) x
Join [CentralDB].[AS].[SSASInfo] y ON x.Rundate = y.DateAdded and X.InstanceName = y.InstanceName) z
inner join(
select Distinct ServerName, InstanceName, Environment, Description, BusinessOwner from  [CentralDB].[Svr].[ServerList]) v ON Z.InstanceName = V.InstanceName AND Z.IsSPUpToDateOnAS = 'False'
Where V.Environment IN (@Environment)
Order by z.InstanceName</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ASVersion">
          <DataField>ASVersion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ASPatchLevel">
          <DataField>ASPatchLevel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ASEdition">
          <DataField>ASEdition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Environ">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <CommandText>SELECT Distinct  [Environment]
  FROM [CentralDB].[Svr].[ServerList]
  Group By Environment
  Order By Environment</CommandText>
      </Query>
      <Fields>
        <Field Name="Environment">
          <DataField>Environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
